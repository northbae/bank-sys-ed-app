<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">

    <changeSet id="table-occ_account_card" author="Lena">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="occ_account_card"/>
            </not>
        </preConditions>
        <createTable tableName="occ_account_card">
            <column name="account_id" type="uuid" defaultValueComputed="gen_random_uuid()">
                <constraints primaryKey="true"/>
            </column>
            <column name="card_id" type="uuid">
                <constraints primaryKey="true" foreignKeyName="occ_account_id_card_id" references="occ_card(id)" deleteCascade="true"/>
            </column>
        </createTable>
        <addUniqueConstraint
                columnNames="account_id, card_id"
                constraintName="account_card"
                tableName="occ_account_card"/>
    </changeSet>

</databaseChangeLog>