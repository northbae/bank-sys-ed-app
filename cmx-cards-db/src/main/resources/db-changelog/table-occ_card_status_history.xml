<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">

    <changeSet id="table-occ_card_status_history" author="Lena">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="occ_card_status_history"/>
            </not>
        </preConditions>
        <createTable tableName="occ_card_status_history">
            <column name="id" type="uuid" defaultValueComputed="gen_random_uuid()">
                <constraints primaryKey="true"/>
            </column>
            <column name="status" type="varchar(30)">
                <constraints nullable="false"/>
            </column>
            <column name="created_at" type="timestamp">
                <constraints nullable="false"/>
            </column>
            <column name="created_by" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="occ_card_id" type="uuid">
                <constraints nullable="false" foreignKeyName="occ_status_history_id_card_id" references="occ_card(id)" deleteCascade="true"/>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>